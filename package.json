{
  "name": "httpcord",
  "version": "0.0.0",
  "description": "Discord HTTP Interaction server",
  "homepage": "https://httpcord.js.org",
  "repository": "git+https://github.com/andre4ik3/httpcord.git",
  "author": "andre4ik3",
  "license": "ISC",
  "packageManager": "yarn@3.2.0",
  "bugs": {
    "url": "https://github.com/andre4ik3/httpcord/issues"
  },
  "main": "dist",
  "scripts": {
    "build": "yarn dlx -p typescript tsc",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "lint": "yarn dlx eslint src/**/*.ts",
    "ci-lint": "yarn lint -f .github/workflows/formatter.js; exit 0"
  },
  "dependencies": {
    "@httpcord/rest": "^0.0.0-9687f6a",
    "discord-api-types": "^0.27.3",
    "fetch-ponyfill": "^7.1.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "collectCoverage": true
  }
